{
  "name": "simple-asset-size-reporter",
  "version": "0.0.1",
  "private": true,
  "description": "A Github Action to easily identify asset size increase (or decrease) in your PRs",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=TEST GITHUB_REPOSITORY=simplabs/ember-asset-size-action mocha -r esm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrysanthos/simple-asset-size-reporter.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Chrysanthos Prodromou",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.2.6",
    "@actions/exec": "1.0.4",
    "@actions/github": "4.0.0",
    "asset-size-reporter": "0.1.0",
    "esm": "3.2.25",
    "pretty-bytes": "5.4.1"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "cross-env": "7.0.2",
    "eslint": "7.10.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-plugin-import": "2.22.1",
    "mocha": "8.1.3"
  }
}
